{
  "name": "@cac/cli",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "bin": {
    "cac": "dist/index.js"
  },
  "scripts": {
    "dev": "tsx src/index.ts",
    "build": "tsup src/index.ts --format esm --target node20 --out-dir dist --clean --banner.bin=\"#!/usr/bin/env node\"",
    "lint": "eslint \"src/**/*.ts\"",
    "test": "vitest run",
    "test:watch": "vitest watch"
  },
  "dependencies": {
    "@cac/core": "workspace:*",
    "@cac/logger": "workspace:*",
    "commander": "^12.1.0",
    "fs-extra": "^11.2.0"
  }
}
